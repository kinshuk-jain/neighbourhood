{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2019",
    "lib": ["ES2019", "DOM"],
    "jsx": "react",
    "moduleResolution": "node",
    "strict": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": false,
    "outDir": "dist"
  }
}
