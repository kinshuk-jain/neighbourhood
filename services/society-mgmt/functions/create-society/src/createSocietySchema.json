{
  "id": "/society",
  "type": "object",
  "properties": {
    "society_type": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": ["general", "residential"]
      }
    },
    "show_directory": {
      "type": "boolean"
    },
    "name": { "type": "string" },
    "address": {
      "type": "object",
      "properties": {
        "postal_code": { "type": "string" },
        "city": { "type": "string" },
        "state": { "type": "string" },
        "country": { "type": "string" },
        "street_address": { "type": "string" }
      },
      "required": ["postal_code", "city", "state", "street_address"]
    }
  },
  "required": ["name", "address", "society_type"]
}
